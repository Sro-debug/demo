      "cell_type": "code",
      "source": [
        "precision_score(predictions[\"Target\"],predictions[\"Predictions\"])\n",
        "predictions[\"Target\"].value_counts()/predictions.shape[0]\n",
        "horizons=[2,5,60,250,1000]\n",
        "new_predictors=[]\n",
        "for horizon in  horizons:\n",
        "  rolling_averages=sp.rolling(horizon).mean()\n",
        "  ratio_column=F\"Close_Ratio_{horizon}\"\n",
        "  sp[ratio_column]=sp[\"Close\"]/rolling_averages[\"Close\"]\n",
        "\n",
        "  trend_column=f\"Trend_{horizon}\"\n",
        "  sp[trend_column]=sp.shift(1).rolling(horizon).sum()[\"Target\"]\n",
        "  new_predictors+=[ratio_column,trend_column]\n",
        "\n",
        "sp=sp.dropna()\n",
        "model=RandomForestClassifier(n_estimators=200,min_samples_split=50,random_state=1)\n",
        "def predict(train,test,predictors,model):\n",
        "  model.fit(train[predictors],train[\"Target\"])\n",
        "  pred=model.predict_proba(test[predictors])[:,1]\n",
        "  pred[pred >=.6]=1\n",
        "  pred[pred <.6]=0\n",
        "  pred=pd.Series(pred,index=test.index,name=\"Predictions\")\n",
        "  combined=pd.concat([test[\"Target\"],pred],axis=1)\n",
        "  return combined\n",
        "#for accurate predictions more optimized\n",
        "predictions=backtest(sp,model,new_predictors)\n",
        "predictions[\"Predictions\"].value_counts()\n",
        "precision_score(predictions[\"Target\"],predictions[\"Predictions\"])\n",
        "\n",
        "\n",
        "\n",
        "\n"
      ],
      
